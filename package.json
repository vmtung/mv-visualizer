{
  "name": "parcel-reactjs",
  "version": "1.0.0",
  "description": "A minimal React.js boilerplate with parcel as bundler",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "parcel ./src/index.html",
    "build": "rm -rf dist && parcel build ./src/index.html --no-source-maps --out-dir dist --public-url ./",
    "watch": "parcel watch ./src/index.html --no-source-maps --out-dir dist --public-url ./",
    "patch": "patch-package"
  },
  "keywords": [
    "parcel",
    "react"
  ],
  "author": "“Robin <“wrobin@gmx.net”> (https://www.robinwieruch.de)",
  "license": "MIT",
  "dependencies": {
    "react": "^16.8.6",
    "react-animated-css": "^1.2.1",
    "react-audio-spectrum": "^0.1.0",
    "react-dom": "^16.8.6"
  },
  "engines": {
    "node": "8.x"
  },
  "devDependencies": {
    "parcel-bundler": "^1.12.4",
    "patch-package": "^6.2.0"
  }
}
